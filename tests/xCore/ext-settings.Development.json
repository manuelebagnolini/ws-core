{
  "$schema": "../../src/modules/json-schema.json#",
  "__$schema": "https://raw.githubusercontent.com/massimodipaolo/ws-core/master/src/modules/json-schema.json",
  "extConfig": {
    "assemblies": {
    },
    "injectors": [
      {
        "priority": 101,
        "services": [
          {
            "serviceType": "Ws.Core.Extensions.Message.IMessage",
            "implementationType": "xCore.NotFoundInjectorServiceMessage",
            "lifeTime": "Transient",
            "overrideIfAlreadyRegistered": false
          }
        ],
        "middlewares": [
          {
            "type": "xCore.Middlewares.RequestCulture"
          }
        ]
      },
      {
        "priority": 301,
        "name": "branch-write-text",
        "middlewares": [
          {
            "type": "xCore.Middlewares.WriteText",
            "map": {
              "pathMatch": "/branch",
              "preserveMatchedPathSegment": true
            }
          }
        ]
      },
      {
        "priority": 405,
        "name": "data-repository-override",
        "services": [
          {
            "serviceType": "Ws.Core.Extensions.Data.IRepository`2[[xCore.Log, xCore],[System.Int32]], Ws.Core.Extensions.Data",
            "implementationType": "Ws.Core.Extensions.Data.Repository.EF.SQLite`2[[xCore.Log, xCore],[System.Int32]], Ws.Core.Extensions.Data.EF.SQLite",
            "lifeTime": "Transient",
            "overrideIfAlreadyRegistered": true
          }
        ]
      },
      {
        "priority": 601,
        "name": "IMessage-on-stereoids",
        "decorators": [
          {
            "serviceType": "Ws.Core.Extensions.Message.IMessage",
            "implementationType": "xCore.Decorators.IMessageLogger"
          },
          {
            "serviceType": "Ws.Core.Extensions.Message.IMessage",
            "implementationType": "xCore.Decorators.IMessageCopy"
          },
          {
            "serviceType": "Ws.Core.Extensions.Message.IMessage",
            "implementationType": "xCore.Decorators.IMessageSignature"
          }
        ]
      }
    ]
  }
}
